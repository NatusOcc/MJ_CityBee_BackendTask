{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CacheProfiles": {
    "Cache5Mins": {
      "Duration": 300,
      "Location": "Any"
    },
    "Cache15sec": {
      "Duration": 15,
      "Location": "Any"
    }
  },
  "AllowedHosts": "*"
}
